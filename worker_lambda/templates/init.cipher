CALL apoc.cypher.runMany(
'
{# 既存のノードを全て削除 #}

MATCH (n) DETACH DELETE n;

{# 制約の設定 #}
CREATE CONSTRAINT c_node_name_is_unique IF NOT EXISTS FOR (node:Node) REQUIRE node.name IS UNIQUE;

{# マスタの代入 #}
UNWIND $csv_params AS csvParam
MERGE (n:Node {name: csvParam.name});

'
,{csv_params: $csv_params}
)
